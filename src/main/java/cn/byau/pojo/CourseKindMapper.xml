  <insert id="save" parameterType="CourseKind" >
    insert into coursekind
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="kindId != null" >
        kind_id,
      </if>
      <if test="kindName != null" >
        kind_name,
      </if>
      <if test="kindRemark != null" >
        kind_remark,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="kindId != null" >
        #{kindId,jdbcType=VARCHAR},
      </if>
      <if test="kindName != null" >
        #{kindName,jdbcType=VARCHAR},
      </if>
      <if test="kindRemark != null" >
        #{kindRemark,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <update id="update" parameterType="CourseKind" >
    update coursekind
    <set >
      <if test="kindName != null" >
        kind_name = #{kindName,jdbcType=VARCHAR},
      </if>
      <if test="kindRemark != null" >
        kind_remark = #{kindRemark,jdbcType=VARCHAR},
      </if>
    </set>
    where kind_id = #{kindId,jdbcType=VARCHAR}
  </update>
